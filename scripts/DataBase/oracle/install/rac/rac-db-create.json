{
    "opType": "target",
    "typeName": "INSTALL",
    "riskName": "MEDIUM",
    "interpreter": "bash",
    "defaultProfile": null,
    "description": "创建OracleRAC DB库(在root用户下执行, 会su到oracle用户执行)",
    "option": [
        {
            "opt": "EXEC_ENV",
            "name": "变量设置",
            "help": "执行前的变量设（使用${}引用作业参数）",
            "type": "textarea",
            "defaultValue": "ORACLE_USER=${ORACLE_USER}\nORACLE_BASE=${ORACLE_BASE}\nORACLE_HOME=${ORACLE_HOME}",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "CV_ASSUME_DISTID",
            "name": "对应OS版本",
            "help": "与OracleLinux对应的Linux版本, RedHat8及以上才需要设置",
            "type": "select",
            "dataSource": {
                "dataList": [
                    {
                        "text": "自动",
                        "value": "AUTO"
                    },
                    {
                        "text": "OL7",
                        "value": "OL7"
                    },
                    {
                        "text": "OEL7.8",
                        "value": "OEL7.8"
                    },
                    {
                        "text": "OL8",
                        "value": "OL8"
                    },
                    {
                        "text": "OL9",
                        "value": "OL9"
                    }
                ],
                "dataType": "static"
            },
            "defaultValue": "AUTO",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "RESPONSE_FILE",
            "name": "静默响应文件",
            "help": "静默安装的响应文件",
            "type": "file",
            "defaultValue": null,
            "required": "true",
            "validate": ""
        },
        {
            "opt": "TEMPLATE_FILE",
            "name": "DB模板文件",
            "help": "创建DB实例的模板文件",
            "type": "file",
            "defaultValue": null,
            "required": "true",
            "validate": ""
        },
        {
            "opt": "DB_NAME",
            "name": "全局DB名",
            "help": "用于作为DB的服务名, DG主备库一致",
            "type": "text",
            "defaultValue": "",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "DB_UNIQUE_NAME",
            "name": "DB唯一名",
            "help": "DB唯一名称, 在DG同步和切换中确定主备库, 为空保持跟DB_NAME一致",
            "type": "text",
            "defaultValue": "",
            "required": "false",
            "validate": ""
        },
        {
            "opt": "NODE_LIST",
            "name": "安装节点",
            "help": "安装DB节点列表(genhossconf工具输出参数dbClusterNodes), 例如: rac1,rac2",
            "type": "text",
            "defaultValue": "",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "CONFIG_TYPE",
            "name": "DB配置类型",
            "help": "DB配置类型",
            "type": "select",
            "dataSource": {
                "dataList": [
                    {
                        "text": "RAC",
                        "value": "RAC"
                    },
                    {
                        "text": "RACONENODE",
                        "value": "RACONENODE"
                    },
                    {
                        "text": "SI",
                        "value": "SI"
                    }
                ],
                "dataType": "static"
            },
            "defaultValue": "RAC",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "DATABASE_TYPE",
            "name": "DB目的类型",
            "help": "DB目的类型",
            "type": "select",
            "dataSource": {
                "dataList": [
                    {
                        "text": "多用途",
                        "value": "MULTIPURPOSE"
                    },
                    {
                        "text": "数据仓库",
                        "value": "DATA_WAREHOUSING"
                    },
                    {
                        "text": "在线处理",
                        "value": "OLTP"
                    }
                ],
                "dataType": "static"
            },
            "defaultValue": "OLTP",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "IS_CDB",
            "name": "是否CDB",
            "help": "是否CDB",
            "type": "select",
            "dataSource": {
                "dataList": [
                    {
                        "text": "是",
                        "value": "true"
                    },
                    {
                        "text": "否",
                        "value": "false"
                    }
                ],
                "dataType": "static"
            },
            "defaultValue": "true",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "PDB_NAME",
            "name": "PDB名称",
            "help": "PDB名称",
            "type": "text",
            "defaultValue": "mypdb",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "CHARSET",
            "name": "字符集",
            "help": "安装DB字符集",
            "type": "select",
            "dataSource": {
                "dataList": [
                    {
                        "text": "UTF8",
                        "value": "AL32UTF8"
                    },
                    {
                        "text": "UTF16",
                        "value": "AL16UTF16"
                    },
                    {
                        "text": "ASCII",
                        "value": "US7ASCII"
                    },
                    {
                        "text": "GBK",
                        "value": "ZHS16GBK"
                    }
                ],
                "dataType": "static"
            },
            "defaultValue": "AL32UTF8",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "CDB_PWD",
            "name": "CDB密码",
            "help": "CDB默认密码",
            "type": "password",
            "defaultValue": "{RC4}a251f3fa848b948696",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "PDB_PWD",
            "name": "PDB密码",
            "help": "PDB默认密码",
            "type": "password",
            "defaultValue": "{RC4}a251f3fa848b948696",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "AUTO_MEMORY",
            "name": "自动内存管理",
            "help": "Oracle自行自动管理内存使用",
            "type": "text",
            "defaultValue": "false",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "MEM_PERCENTAGE",
            "name": "内存百分比",
            "help": "占用内存百分比, 如果设置了自动内存管理, 则此设置会失效",
            "type": "text",
            "defaultValue": "50",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "STORAGE_TYPE",
            "name": "存储类型",
            "help": "存储类型",
            "type": "select",
            "dataSource": {
                "dataList": [
                    {
                        "text": "ASM",
                        "value": "ASM"
                    },
                    {
                        "text": "文件系统",
                        "value": "FS"
                    }
                ],
                "dataType": "static"
            },
            "defaultValue": "ASM",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "ARCH_DISKGROUP",
            "name": "归档磁盘组",
            "help": "用于存放REDO, 归档日志, 闪回等数据的磁盘组名称",
            "type": "text",
            "defaultValue": "ARCH",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "DATA_DISKGROUP",
            "name": "业务磁盘组",
            "help": "用于存放业务数据的磁盘组名称",
            "type": "text",
            "defaultValue": "DATA",
            "required": "true",
            "validate": ""
        },
        {
            "opt": "INIT_PARAMS",
            "name": "INIT参数",
            "help": "dbca需要的initParams",
            "type": "textarea",
            "defaultValue": "db_unique_name={DB_UNIQUE_NAME}\ncompatible=19.0.0\ndb_block_size=8192BYTES\nsga_target=768MB\npga_aggregate_target=256MB\nprocesses=300\nopen_cursors=300\ncluster_database=true\ndb_recovery_file_dest_size=10GB",
            "required": "true",
            "validate": ""
        }
    ],
    "output": []
}